rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /profile_pics_forum/{username}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /livechat_images/{userId}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}